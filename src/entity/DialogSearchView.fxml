<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="m_rootPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="entity.DialogSearchController">
   <center>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="14.0" layoutY="38.0" text="Rechercher sur base de :" />
            <ComboBox fx:id="m_comboType" layoutX="164.0" layoutY="34.0" prefWidth="150.0" />
            <Label layoutX="14.0" layoutY="95.0" text="A rechercher:" />
            <TextField fx:id="m_textSearch" layoutX="165.0" layoutY="91.0" prefHeight="25.0" prefWidth="300.0" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#OnSearch" text="Rechercher" />
            <Button mnemonicParsing="false" onAction="#OnCancel" text="Annuler" />
        </items>
      </ToolBar>
   </bottom>
</BorderPane>
